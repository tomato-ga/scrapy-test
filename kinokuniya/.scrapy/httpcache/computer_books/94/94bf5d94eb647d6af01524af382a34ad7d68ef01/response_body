function insertCartSeries(form) {
    var cnt = 1;
    for(var i = 0; i < form.elements.length; i++) {
        if(form.elements[i].type == "checkbox" && form.elements[i].name.indexOf("GOODS_INFO_SET") != -1 && form.elements[i].checked) {
            return true;
        }
    }
    alert("カートに入れる商品が選択されていません。");
    return false;
}
</script>

